{
  "rules": {
    "no-unused-vars": 1,
    "no-undef": 1,
    "no-const-assign": 1,
    "@lightscript/variables": 1,
    "@lightscript/illegalInIife": 1,
    "@lightscript/implicitImports": 1,
    "@lightscript/matchRequiresElse": 1,
    "@lightscript/unnecessaryConst": 1,
    "@lightscript/unnecessarySemi": 1,
    "@lightscript/unnecessaryComma": 1
  }
}
